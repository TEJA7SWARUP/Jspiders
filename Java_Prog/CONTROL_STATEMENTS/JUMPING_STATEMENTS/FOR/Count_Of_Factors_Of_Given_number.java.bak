import java.util.Scanner;
class Count_Of_Factors_Of_Given_number
{
	public static void main(String[] args) 
	{
		Scanner se = new Scanner(System.in);
		System.out.println("Enter last digit to print : ");
		int num = se.nextInt();
		int count=0;
		for (int i = 1;i<num ; i++)
		{
			if(num%i == 0)
			{
				count++;
			}
		}
		System.out.println(count);
		
	}
}
